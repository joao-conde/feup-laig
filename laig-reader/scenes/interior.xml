        <ambient r="0.0" g="0.0" b="0.0" a="1" />         
        <emission r="0.0" g="0.0" b="0.0" a="1" />       
    </MATERIAL>


    <!-- Kitchen Walls Material -->
    <MATERIAL id="kitchenWallsMaterial">                       
        <shininess value="100" />
        <specular r=".3" g="0" b="0.1" a="1" />       
        <diffuse r="0.07" g="0" b="0.1" a="1" />       
