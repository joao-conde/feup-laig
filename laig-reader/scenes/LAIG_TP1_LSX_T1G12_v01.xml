        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>

</LIGHTS>

<!-- Textures -->
<TEXTURES>
    <TEXTURE id="water">
      <file path="images/water.jpg" />
      <amplif_factor s="0.5" t="0.5" />
    </TEXTURE>

	<!-- Tree trunk -->
    <TEXTURE id="cup">
        <file path="images/cup.jpg" />
        <amplif_factor s="2" t="2" />
    </TEXTURE>

