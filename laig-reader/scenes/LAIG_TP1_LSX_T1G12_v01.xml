    <TEXTURE id="tiles">
        <file path="images/tiles.jpg" />
        <amplif_factor s="0.2" t="0.2" />
    </TEXTURE>

    <TEXTURE id="floor">
        <file path="images/floor.jpg" />
        <amplif_factor s="0.4" t="0.4" />
    </TEXTURE>



    <!-- Rocks -->
    <TEXTURE id="grass">
        <file path="images/grass.jpg" />
        <amplif_factor s="0.3" t="0.3" />
    </TEXTURE>

    <!-- Glass work -->
    <TEXTURE id="ball">
        <file path="images/ball.jpg" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Street bench -->
    <TEXTURE id="wood">
        <file path="images/wood.jpg" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <!-- Poll ball -->
    <TEXTURE id="pink">
